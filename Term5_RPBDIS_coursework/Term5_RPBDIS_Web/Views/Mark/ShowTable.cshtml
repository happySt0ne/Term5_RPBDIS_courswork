@using Term5_RPBDIS_library.models.tables;
@{
    var data = ViewBag.Data as List<Mark>;
}

<html>
<head>
    <title>Оценки</title>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
</head>
<body>
    <h1>Список оценок</h1>
    <table border="1">
        <tr>
            <th>Код</th>
            <th>Оценка</th>
        </tr>

        @foreach (Mark mark in data) {
            <tr>
                <td>@mark.ID</td>
                <td>@mark.Value</td>
            </tr>
        }
    </table>
</body>
</html>
