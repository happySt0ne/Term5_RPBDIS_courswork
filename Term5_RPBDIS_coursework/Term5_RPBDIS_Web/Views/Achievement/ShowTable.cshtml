@using Term5_RPBDIS_library.models.tables;
@{
    var data = ViewBag.data as List<Achievement>;
}

<html>
<head>
    <title>Достижения</title>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
</head>
<body>
    <h1>Список достижений</h1>
    <table border="1">
        <tr>
            <th>Код</th>
            <th>Текст</th>
        </tr>

        @foreach (Achievement a in data) {
            <tr>
                <td>@a.ID</td>
                <td>@a.Text</td>
            </tr>
        }
    </table>
</body>
</html>
