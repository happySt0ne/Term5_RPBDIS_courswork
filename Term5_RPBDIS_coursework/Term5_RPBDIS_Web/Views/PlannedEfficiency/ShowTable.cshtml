@using Term5_RPBDIS_library.models.tables;
@{
    var data = ViewBag.Data as List<PlannedEfficiency>;
}

<html>
<head>
    <title>Планы</title>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
</head>
<body>
    <h1>Список планов</h1>
    <table border="1">
        <tr>
            <th>Код</th>
            <th>Эффективность</th>
            <th>Начальная дата</th>
            <th>Конечная дата</th>
        </tr>

        @foreach (PlannedEfficiency plannedEfficiency in data) {
            <tr>
                <td>@plannedEfficiency.ID</td>
                <td>@plannedEfficiency.Efficiecy</td>
                <td>@plannedEfficiency.Date?.StartDate</td>
                <td>@plannedEfficiency.Date?.EndDate</td>
            </tr>
        }
    </table>
</body>
</html>
